	"eslintConfig": {
		"env": {
			"browser": true,
			"commonjs": true,
			"es6": true,
			"node": true
		},
		"parserOptions": {
			"ecmaFeatures": {
				"jsx": true
			},
			"sourceType": "module"
		},
		"rules": {
			"no-const-assign": "error",
			"no-this-before-super": "warn",
			"no-undef": "warn",
			"no-unreachable": "warn",
			"no-unused-vars": [
				"error",
				{
					"args": "none"
				}
			],
			"constructor-super": "warn",
			"valid-typeof": "warn"
		}
	}
